<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef ParentName="MotorBuildingBase">
        <defName>Basic_Civilian_Car</defName>
        <label>Civilian Car</label>
        <description>A simple car. Many people use this to commute to work.</description>
        <graphicData>
            <texPath>yy/Dacia_east</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <drawSize>(5,5)</drawSize>
        </graphicData>
        <uiIconPath>yy/Dacia_east</uiIconPath>
        <size>(4,2)</size>
        <costList>
            <Steel>200</Steel>
            <ComponentIndustrial>10</ComponentIndustrial>
        </costList>
        <statBases>
            <MaxHitPoints>300</MaxHitPoints>
            <Beauty>10</Beauty>
            <WorkToBuild>2500</WorkToBuild>
            <Flammability>0.55</Flammability>
        </statBases>
        <researchPrerequisites>
            <li>RHResearch_Vehicles</li>
        </researchPrerequisites>
    </ThingDef>
	
